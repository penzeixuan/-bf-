	PremiumOnly = false
})

Tab:AddToggle({
	Name = "开启",
	Default = false,
	Callback = function(Value)
		print(Value)
			TPF = t
			while wait() do
			   if TPF then
				 for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
